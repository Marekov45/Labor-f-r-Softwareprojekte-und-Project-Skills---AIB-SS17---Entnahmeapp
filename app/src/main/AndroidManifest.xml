                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LoginActivity"
                android:configChanges="orientation"
                android:label="@string/title_activity_login"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.scan.ScanActivity"
                android:configChanges="orientation"
                android:label="@string/title_activity_login"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LaborGui"
                android:configChanges="orientation"
                android:label="@string/title_activity_labor"
                android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LoginActivity"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LagerRueckgabeGUI"
                android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LoginActivity"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.popup.PopPicklist"
                android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.PickListActivity"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.CustomTheme">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.popup.PopSanger"
                android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LastProcessedListActivity"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.CustomTheme">
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.popup.PopTracking"
                android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LaborGui"
                android:screenOrientation="portrait"
                android:theme="@style/AppTheme.CustomTheme">
        </activity>

        <activity android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.popup.PopReturn"
                  android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LagerRueckgabeGUI"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme.CustomTheme">
        </activity>
        <activity android:name="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LastProcessedListActivity"
                  android:parentActivityName="client_aib_labswp_2017_ss_entnahmeapp.View.view.guis.LoginActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>