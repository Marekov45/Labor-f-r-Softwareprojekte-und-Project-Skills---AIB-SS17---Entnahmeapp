<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="client.aib_labswp_2017_ss_entnahmeapp">


    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.LoginActivity"
                android:configChanges="orientation"
                android:screenOrientation="portrait"
                android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.PrimerList"
                android:configChanges="orientation"
                android:screenOrientation="portrait"
                android:label="@string/title_activity_login"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name="client_aib_labswp_2017_ss_entnahmeapp.View.ScanActivity"
                android:configChanges="orientation"
                android:screenOrientation="portrait"
                android:label="@string/title_activity_login"
                android:theme="@style/AppTheme.NoActionBar">

        </activity>
    </application>

</manifest>